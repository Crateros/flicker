<% if(alerts.error) { %>
  <% alerts.error.forEach(function(msg) { %>
    <div class="alert alert-danger" id="danger-alert"><button type="button" class="close" data-dismiss="alert">x</button><%= msg %></div>
<%  }); %>
<% } %>
<% if(alerts.success) { %>
  <% alerts.success.forEach(function(msg) { %>
    <div class="alert alert-success" id="success-alert"><%= msg %></div>
<%  }); %>
<% } %>
